{
  "name": "riven-az-pc",
  "version": "1.0.0",
  "description": "My geometric Nuxt.js project",
  "author": "tye",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --max-old-space-size=8192 server/index.js --watch server",
    "devLangLocal": "cross-env NODE_ENV=development, NODE_LANG=local nodemon server/index.js --watch server",
    "test-min": "cross-env NODE_ENV=production NODE_ENV_TEST=development node --max-http-header-size=31000 server/index.js",
    "test": "cross-env NODE_ENV=development node --max-old-space-size=4096 --max-http-header-size=31000 server/index.js",
    "build": "node --max-old-space-size=5120 node_modules/nuxt/bin/nuxt.js build",
    "production": "cross-env NODE_ENV=production node --max-http-header-size=31000 server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore ."
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/proxy": "^1.3.3",
    "@nuxtjs/style-resources": "^1.0.0",
    "@sentry/node": "^7.3.1",
    "@sentry/tracing": "^7.4.1",
    "@sentry/vue": "^7.3.1",
    "array-includes": "^3.1.1",
    "async-validator": "^3.2.3",
    "axios": "^0.20.0",
    "axios-extensions": "^3.1.3",
    "cheerio": "^1.0.0-rc.12",
    "colorthief": "^2.3.2",
    "core-js": "2.6.5",
    "cross-env": "^5.2.0",
    "dayjs": "^1.11.9",
    "echarts": "^4.8.0",
    "elastic-apm-node": "^3.9.0",
    "get-json": "^1.0.1",
    "intersection-observer": "^0.12.0",
    "js-base64": "^3.4.5",
    "js-md5": "^0.7.3",
    "koa": "^2.6.2",
    "koa-bodyparser": "^4.3.0",
    "koa-log4": "^2.1.0",
    "lazysizes": "^5.2.0-beta1",
    "lru-cache": "^5.1.1",
    "moment": "^2.24.0",
    "newrelic": "^8.11.1",
    "nuxt": "~2.11.0",
    "nuxt-ssr-cache": "1.5.2",
    "qs": "^6.9.1",
    "redis": "^3.0.2",
    "superagent": "^8.1.2",
    "useragent": "^2.3.0",
    "util": "^0.12.3",
    "uuid": "^8.3.2",
    "vant": "^2.10.1",
    "velocity-animate": "^1.5.2",
    "vue-awesome-swiper": "^3.1.3",
    "vue-cleave-component": "^2.1.3",
    "vue-clipboard2": "^0.3.1",
    "vue-lazyload": "^1.3.3",
    "vue-slider-component": "^3.1.1",
    "vue2-datepicker": "^3.3.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "1.0.1",
    "@nuxtjs/eslint-module": "1.0.0",
    "@sentry/webpack-plugin": "^1.18.9",
    "@vue/runtime-dom": "^3.3.8",
    "babel-eslint": "10.0.1",
    "eslint": "6.1.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-nuxt": "0.4.2",
    "eslint-plugin-prettier": "3.0.1",
    "husky": "^2.6.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "lint-staged": "^8.2.1",
    "nodemon": "^1.18.9",
    "nuxt-build-optimisations": "^1.0.7",
    "prettier": "^1.16.4",
    "svg-sprite-loader": "^5.0.0"
  }
